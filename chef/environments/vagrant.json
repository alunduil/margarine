{
  "name": "vagrant",
  "description": "The environment for margarine under vagrant.",

  "json_class": "Chef::Environment",
  "chef_type": "environment",

  "default_attributes": {},

  "override_attributes": {
    "margarine": {
      "install": {
        "method": "vagrant",
        "path": "/vagrant"
      },
      "logging": { "default": true },
      "service": {
        "hostname": "0.0.0.0",
        "provider": "flask"
      },
      "flask": { "debug": true }
    },
    "queue": {
      "password": "6760de0e-2f27-430b-b5b8-ce55ed394b93"
    },
    "datastore": {
      "password": "c9a4272f-e4b2-4a25-98bc-0c6b44ce34d8"
    },
    "rabbitmq": {
      "enabled_plugins": [ "rabbitmq_management" ]
      "enabled_users": [
        {
          "name": "margarine",
          "password": "6760de0e-2f27-430b-b5b8-ce55ed394b93",
          "tag": "administrator",
          "rights": [
            {
              "vhost": "/",
              "conf": ".*",
              "write": ".*",
              "read": ".*"
            }
          ]
        }
      ],
      "disabled_users": [ "guest" ]
    }
  },

  "cookbook_versions": {}
}
